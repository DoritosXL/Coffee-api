{
  "name": "coffee-places-api",
  "version": "1.0.0",
  "description": "Coffee Places API built with Fastify and Zod",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "vercel-build": "npm run build",
    "sync:osm": "ts-node src/scripts/syncOSM.ts",
    "sync:google": "ts-node src/scripts/syncGooglePlaces.ts",
    "db:generate": "prisma generate",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push"
  },
  "keywords": [
    "fastify",
    "typescript",
    "zod",
    "rest-api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.2.0",
    "@googlemaps/google-maps-services-js": "^3.3.50",
    "@prisma/client": "^6.18.0",
    "@vercel/node": "^3.0.7",
    "axios": "^1.13.1",
    "dotenv": "^17.2.3",
    "fastify": "^4.24.3",
    "prisma": "^6.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
